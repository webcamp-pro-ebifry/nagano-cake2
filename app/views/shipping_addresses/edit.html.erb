<div class="container">
	<div class="row">
		<div class="col-lg-2"></div>

		<h3>配送先編集</h3>
		<%= form_for(@shipping) do |f| %>
			<div class="">
				<%=f.label :郵便番号%>
				<%=f.text_field :postcode %>
			</div>

			<div class="">
				<%=f.label :住所%>
				<%=f.text_field :address %>
			</div>

			<div class="">
				<%=f.label :宛名%>
				<%=f.text_field :name%>
			</div>
	</div>
	<div class="row">
		<div class="col-lg-5"></div>
		<div class="col-lg-2"><%=f.submit "編集する", class:"btn btn-primary btn-md btn-base-1"%></div>
	</div>
<% end %>
</div>
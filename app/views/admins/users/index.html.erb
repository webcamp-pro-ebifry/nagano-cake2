<h1>Admins::Users#index</h1>
<p>Find me in app/views/admins/users/index.html.erb</p>
<div class="cotainer">

<div class="row">

<div class="col-xs-9">

<h2>会員一覧</h2>

<table class="table">
	<thead>
		<tr>
			<th>ID</th>
			<th>氏名</th>
			<th>メールアドレス</th>
			<th>ステータス</th>
		</tr>
	</thead>

	<tbody>
		<% @users.each do |user| %>
			<tr>
				<td><%= user.id %></td>
				<td>
                	<%= link_to admins_user_path(user.id) do %>
					<%= user.firstname %>
					<%= user.lastname %>
					<% end %>
		    	</td>
            	<td><%= user.email %></td>
            	<td>ステータス</td>
			</tr>
		<% end %>
	</tbody>
</table>
<%= paginate @users %>
</div>
</div>
</div>
<h1>Admins::Users#edit</h1>
<p>Find me in app/views/admins/users/edit.html.erb</p>



<%= form_for(@user, url: admins_user_path(@user)) do |f| %>
<table>
   	<tr>
   		<th>名前</th>
   			<td><span>(姓) </span></td>
			<td>
				<%= f.text_field :firstname %>
				<span>　(名)　</span>
				<%= f.text_field :lastname %>
			</td>
	</tr>
	<tr>
		<th>フリガナ</th>
			<td><span>(セイ) </span></td>
		<td>
			<%= f.text_field :firstname_ruby %>
			<span>　(メイ) </span>
			<%= f.text_field :lastname_ruby %>
		</td>
	</tr>
	<tr>
		<th>メールアドレス</th>
		<td></td>
		<td>
			<%= f.email_field :email %>
		<td>
	</tr>
	<tr>
		<th>郵便番号(ハイフンなし)</th>
		<td></td>
		<td>
			<%= f.text_field :postal_code %>
		</td>
	</tr>
	<tr>
		<th>住所</th>
		<td></td>
		<td>
			<%= f.text_field :address %>
		</td>
	</tr>
	<tr>
		<th>電話番号(ハイフンなし)</th>
		<td></td>
		<td>
			<%= f.phone_field :phone_number %>
		</td>
	</tr>
</table>

<div class="row">
	<div class="col-lg-2">
	<%= f.submit "登録内容を保存する", class: "btn btn-info" %>
	</div>
<% end %>
